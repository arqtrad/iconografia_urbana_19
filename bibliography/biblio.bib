@article{zanini:2008pertencimento13,
  title = {Pertencimento Étnico e Territorialidade: Italianos Na Região Central Do {{Rio Grande}} Do {{Sul}} ({{Brasil}})},
  shorttitle = {Pertencimento Étnico e Territorialidade},
  author = {Zanini, Maria Catarina},
  date = {2008},
  journaltitle = {Redes (Santa Cruz do Sul. Online)},
  volume = {13},
  pages = {140--163},
  issn = {1982-6745},
  doi = {10.17058/redes.v13i3.743},
  abstract = {Este artigo tem por objetivo apresentar algumas reflexões acerca da relação entre identidade étnica e pertencimento territorial entre imigrantes e descendentes de imigrantes italianos para a região central do Rio Grande do Sul ao longo de mais de século de processo colonizador. Partindo de pesquisa etnográfica, especialmente problematizando como estas questões tem sido tratadas ao longo do tempo, pretende-se salientar o quanto são temáticas abertas e se refazem constantemente enquanto fronteiras adscritivas importantes para a delimitação das identidades e dos pertencimentos. Inicialmente um processo migratório formado por camponeses (católicos e pobres em sua grande maioria) foi, ao longo do contato com  a situação regional, provocando uma migração urbana de alguns para Santa Maria e região. Posteriormente, devido às questões relativas à sucessão hereditária da terra, aumento dos membros familiares e ao pequeno tamanho dos lotes, impulsionando uma migração para outros estados brasileiros, como Santa Catarina, Paraná, Mato Grosso, Bahia, criando, desta forma, novas frentes de expansão agrícolas no país. Toda esta movimentação espacial faz parte das memórias familiares e dos itinerários geracionais, destacando o quanto, para os descendentes de imigrantes italianos, a migração tem sido algo presente historicamente em suas existências.},
  keywords = {Identidade étnica,italianos,migração},
  langid = {english},
  number = {3}
}
